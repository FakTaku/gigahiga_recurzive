{
  "appId": "mail.google.com",
  "version": "2025.01.0",
  "intents": [
    { "id": "palette.open", "title": "Open Command Palette" },
    { "id": "compose.open", "title": "Compose" },
    { "id": "message.send", "title": "Send Email" },
    { "id": "draft.save", "title": "Save Draft" },
    { "id": "nav.search", "title": "Search" }
  ],
  "scopes": [
    {
      "name": "global",
      "routes": ["/*"],
      "bindings": [
        { "key": "/", "intent": "nav.search", "priority": 80, "source": "developer_default" },
        { "key": "Ctrl+K", "intent": "palette.open", "platform": ["win","linux"], "priority": 90, "source": "developer_default" },
        { "key": "Meta+K", "intent": "palette.open", "platform": ["mac"], "priority": 90, "source": "developer_default" },
        { "key": "Ctrl+N", "intent": "compose.open", "platform": ["win","linux"], "priority": 85, "source": "developer_default" },
        { "key": "Meta+N", "intent": "compose.open", "platform": ["mac"], "priority": 85, "source": "developer_default" }
      ]
    },
    {
      "name": "compose",
      "routes": ["/compose", "/u/*/compose/*"],
      "bindings": [
        { "key": "Ctrl+Enter", "intent": "message.send", "priority": 100, "source": "developer_default" },
        { "key": "Meta+Enter", "intent": "message.send", "priority": 100, "source": "developer_default", "platform": ["mac"] },
        { "key": "Ctrl+S", "intent": "draft.save", "priority": 95, "source": "developer_default" }
      ]
    }
  ],
  "signature": "DEV_ONLY_UNSIGNED"
}


